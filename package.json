{
  "name": "cypress-do-zero-a-nuvem",
  "version": "1.0.0",
  "description": "Curso 'Cypress, do Zero à Nuvem' da Escola Talking About Testing.",
  "main": "src/index.html",
  "scripts": {
    "test": "cypress run",
    "allure-chro": "cypress run --browser chrome --env allure=true",
    "allure-fire": "npx cypress run --browser firefox --env allure=true",
    "allure:repo": "allure generate allure-results --clean -o allure-report",
    "allure:clea": "rm -r allure-results/ allure-report cypress/screenshots || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/cypress-do-zero-a-nuvem.git"
  },
  "keywords": [
    "Testing",
    "Cypress.io",
    "Talking About Testing",
    "Cypress, do Zero à Nuvem",
    "Cypress Cloud",
    "GitHub Actions",
    "Escola TAT"
  ],
  "author": "Walmyr <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "homepage": "https://github.com/wlsf82/cypress-do-zero-a-nuvem#readme",
  "bugs": {
    "url": "https://github.com/wlsf82/cypress-do-zero-a-nuvem/issues"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.0",
    "cypress": "^14.3.3"
  }
}
